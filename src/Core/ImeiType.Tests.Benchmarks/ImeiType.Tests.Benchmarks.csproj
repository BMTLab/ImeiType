<!-- ./src/Core/ImeiType.Tests.Benchmarks/ImeiType.Tests.Benchmarks.csproj -->
<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup Label="FrameworkConfig">
        <OutputType>Exe</OutputType>
        <TargetFrameworks>net9.0;net8.0;net6.0</TargetFrameworks>
        <Configurations>Release</Configurations>
        <IsTestProject>false</IsTestProject>
        <NoWarn>$(NoWarn);CS7022</NoWarn> <!-- The entry point of the program is global code; ignoring 'AutoGeneratedProgram.Main(string[])' entry point -->
        <NoWarn>$(NoWarn);S1118</NoWarn> <!-- Utility classes should not have public constructors -->
    </PropertyGroup>

    <PropertyGroup Label="BuildConfig">
        <ErrorReport>none</ErrorReport>
        <CheckForOverflowUnderflow>false</CheckForOverflowUnderflow>
        <DebugType>none</DebugType>
        <DebugSymbols>false</DebugSymbols>
        <Optimize>true</Optimize>
    </PropertyGroup>

    <ItemGroup Label="ProjectReferences">
        <ProjectReference Include="..\ImeiType\ImeiType.csproj" />
    </ItemGroup>

    <ItemGroup Label="TestReferences">
        <PackageReference Include="BenchmarkDotNet"/>
    </ItemGroup>

</Project>